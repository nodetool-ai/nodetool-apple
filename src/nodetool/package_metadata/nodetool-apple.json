    {
      "title": "Capture Screen",
      "description": "Capture screen content via PyObjC\n    screen, automation, macos, media",
      "namespace": "apple.screen",
      "node_type": "apple.screen.CaptureScreen",
      "properties": [
        {
          "name": "whole_screen",
          "type": {
            "type": "bool"
          },
          "default": true,
          "title": "Whole Screen",
          "description": "Capture the whole screen"
        },
        {
          "name": "x",
          "type": {
            "type": "int"
          },
          "default": 0,
          "title": "X",
          "description": "X coordinate of the region to capture"
        },
        {
          "name": "y",
          "type": {
            "type": "int"
          },
          "default": 0,
          "title": "Y",
          "description": "Y coordinate of the region to capture"
        },
        {
          "name": "width",
          "type": {
            "type": "int"
          },
          "default": 1920,
          "title": "Width",
          "description": "Width of the region to capture"
        },
        {
          "name": "height",
          "type": {
            "type": "int"
          },
          "default": 1080,
          "title": "Height",
          "description": "Height of the region to capture"
        }
      ],
      "outputs": [
        {
          "type": {
            "type": "image"
          },
          "name": "output"
        }
      ],
      "basic_fields": [
        "whole_screen",
        "x",
        "y",
        "width",
        "height"
      ]
    },
    {
      "title": "Spotlight Metadata",
      "description": "Read Spotlight metadata for a file using `mdls`.\n    spotlight, metadata, macos, retrieval\n\n    Use cases:\n    - Fetch title/authors/kind for retrieved files\n    - Inspect tags, content type, and dates\n    - Build structured context for agents",
      "namespace": "apple.spotlight",
      "node_type": "apple.spotlight.SpotlightMetadata",
      "properties": [
        {
          "name": "file",
          "type": {
            "type": "asset"
          },
          "default": null,
          "title": "File",
          "description": "File to inspect (usually from SpotlightSearch)",
          "required": true
        },
        {
          "name": "keys",
          "type": {
            "type": "list",
            "type_args": [
              {
                "type": "str"
              }
            ]
          },
          "default": null,
          "title": "Keys",
          "description": "Optional mdls keys (e.g. ['kMDItemKind','kMDItemContentType']); empty means all",
          "required": true
        }
      ],
      "outputs": [
        {
          "type": {
            "type": "dict",
            "type_args": [
              {
                "type": "str"
              },
              {
                "type": "any"
              }
            ]
          },
          "name": "output"
        }
      ],
      "basic_fields": [
        "file",
        "keys"
      ]
    },
    {
