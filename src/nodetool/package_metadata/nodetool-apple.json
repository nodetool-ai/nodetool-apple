    {
      "title": "OCRImage",
      "description": "Extract text from an image using Apple's Vision framework (VNRecognizeTextRequest).\n    vision, ocr, macos, automation\n\n    Use cases:\n    - Screenshot \u2192 OCR \u2192 summarize / classify\n    - Read text from receipts, dialogs, error messages\n    - Turn UI text into reminders/notes/messages",
      "namespace": "apple.vision",
      "node_type": "apple.vision.OCRImage",
      "properties": [
        {
          "name": "image",
          "type": {
            "type": "image"
          },
          "default": {
            "type": "image",
            "uri": "",
            "asset_id": null,
            "data": null,
            "metadata": null
          },
          "title": "Image",
          "description": "Image to run OCR on"
        },
        {
          "name": "recognition_level",
          "type": {
            "type": "enum",
            "values": [
              "fast",
              "accurate"
            ],
            "type_name": "nodetool.nodes.apple.vision.OCRImage.RecognitionLevel"
          },
          "default": "accurate",
          "title": "Recognition Level",
          "description": "OCR speed/accuracy tradeoff"
        },
        {
          "name": "languages",
          "type": {
            "type": "list",
            "type_args": [
              {
                "type": "str"
              }
            ]
          },
          "default": [],
          "title": "Languages",
          "description": "Optional BCP-47 language codes (e.g. ['en-US','de-DE'])"
        },
        {
          "name": "uses_language_correction",
          "type": {
            "type": "bool"
          },
          "default": true,
          "title": "Uses Language Correction",
          "description": "Enable language correction"
        },
        {
          "name": "min_confidence",
          "type": {
            "type": "float"
          },
          "default": 0.0,
          "title": "Min Confidence",
          "description": "Drop recognized strings below this confidence",
          "min": 0.0,
          "max": 1.0
        }
      ],
      "outputs": [
        {
          "type": {
            "type": "str"
          },
          "name": "output"
        }
      ],
      "basic_fields": [
        "image",
        "recognition_level",
        "languages",
        "min_confidence"
      ]
    },
